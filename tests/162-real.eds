#include "stdio.h"
#include "math.h"

double x, y;
int i, j;

void main ()
{
	writeString("Enter int I: ");
	i = readInteger();
	writeString("Enter int J: ");
	j = readInteger();
	x = (double) i / (double) j;
	writeString("I / J = "); writeReal(x);
	y = atan(x);
	writeString("\nArctan(I/J) = ");
	writeReal(y);
	writeString("\n");
}

